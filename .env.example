# Application
APP_ENV=development
APP_NAME=GlobalID
DEBUG=true
LOG_LEVEL=INFO

# Database
DATABASE_URL=postgresql+asyncpg://globalid:globalid_dev_password@localhost:5432/globalid
DATABASE_URL_SYNC=postgresql://globalid:globalid_dev_password@localhost:5432/globalid

# Redis
REDIS_URL=redis://localhost:6379/0

# Qdrant Vector Database
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
QDRANT_COLLECTION_NAME=diseases

# AI Models
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# AI Configuration
DEFAULT_MODEL=gpt-4-turbo
FALLBACK_MODEL=gpt-3.5-turbo
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2000
MAX_RETRIES=3

# Feature Flags
ENABLE_AI_CACHE=true
ENABLE_RATE_LIMITING=true
ENABLE_VECTOR_SEARCH=true

# Rate Limiting
RATE_LIMIT_REQUESTS=50
RATE_LIMIT_WINDOW=60

# Cache
CACHE_TTL=3600

# Paths
DATA_DIR=./data
LOG_DIR=./logs
CONFIG_DIR=./configs

# Monitoring (Optional)
SENTRY_DSN=
